const app = require('../app'),
    http = require('http'),
    config = require('../conf/config.json'),
    vamsModule = require('../module');

app.set('port', config.this.port);

let server = http.createServer(app);

server.listen(config.this.port, async () => {
    console.log('req server start');
    await vamsModule.videoModule.checkVideos();
    
    // vamsModule.requestModule.requestSRorNOTI(vamsModule.dataModule.makeSRorNOTI());

    // vamsModule.requestModule.requestKIDS(vamsModule.dataModule.makeKIDS());

    // vamsModule.requestModule.requestPOSTER(vamsModule.dataModule.makePOSTER());

    // vamsModule.requestModule.requestCI(vamsModule.dataModule.makeCI());
    
    console.log('req process end');
});
